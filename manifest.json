{
  "manifest_version": 3,
  "name": "Sibilance_Shredder",
  "version": "1.2",
  "description": "extension for reducing sibilance in speech.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}